{
  "manifest_version": 2,
  "name": "Your Story",
  "description": "Visualize Your Browsing History",
  "version": "0.0.0.2",
  "permissions": [
    "history", "tabs", "http://*/",  "identity", "storage", "background", "alarms"
  ],
  "background": {
    "scripts": ["event.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [],
      "js": ["content.js"]
    }
  ],
  "chrome_url_overrides" : {
    "newtab": "./index.html"
  },
  "icons" : {
    "16": "./assets/logos/logo-16.png",
    "48": "./assets/logos/logo-48.png",
    "128": "./assets/logos/logo-128.png"
  },
  "oauth2": {
     "client_id": "799894733477-2hu801jg2rd8pidf11dv6cfb5fu4kbfs.apps.googleusercontent.com",
    "scopes": [
                "https://www.googleapis.com/auth/userinfo.email"
        ]
  }
}
